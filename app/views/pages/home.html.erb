 <div class="row">
   <% @rooms.each do |room| %>
     <div class="col-md-4">
       <div class="panel panel-default">
         <div class="panel-heading preview">
           <%= image_tag room.cover_photo(:small) %>
         </div>
         <div class="panel body">
           <%= link_to room.listing_name, room %>
         </div>
       </div>
     </div>
   <% end %>
 </div>

<!-- GETBACK - Adjust and add timepicker -->
 <script>
   $('#start_date').datepicker({
     dateFormat: 'dd-mm-yy',
     minDate: 0,
     maxDate: '1d',
     onSelect: function(selected) {
       $('#end_date').datepicker("option", "minDate", selected);
       $('#end_date').attr("disabled", false);
     }
   });
   $('#end_date').datepicker({
     dateFormat: 'dd-mm-yy',
     minDate: 0,
     maxDate: '1d',
     onSelect: function(selected) {
       $('#end_date').datepicker("option", "maxDate", selected);
     }
   });

   $('#start_time').timepicker({
    timeFormat: 'hh:mm',
    minTime: 0.5,
    maxTime: '23',
    onSelect: function(selected) {
      $('#end_time').datepicker("option", "minTime", selected);
      $('#end_time').attr("disabled", false);
    }
   });
   $('#end_time').timepicker({
    timeFormat: 'hh:mm',
    minTime: 0.5,
    maxTime: '23',
    onSelect: function(selected) {
      $('#start_time').datepicker("option", "maxTime", selected);
    }
   });
 </script>
